<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Cm_layOut_SubCategory_CategoryScteen"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/Cm_image_SubCategory_CategoryScteen"
        android:layout_width="@dimen/_70sdp"
        android:layout_height="@dimen/_80sdp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/Cm_Title_SubCategory_CategoryScteen"
        style="@style/SubHeaderTextViewFont"
        android:layout_width="@dimen/_60sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp"
        android:fontFamily="sans-serif-thin"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Cm_image_SubCategory_CategoryScteen"
        app:layout_constraintStart_toStartOf="@+id/Cm_image_SubCategory_CategoryScteen"
        app:layout_constraintTop_toBottomOf="@+id/Cm_image_SubCategory_CategoryScteen"
        tools:text="TextView" />

    <com.github.ybq.android.spinkit.SpinKitView
        android:id="@+id/cm_spin_kit_SubCategoris"
        style="@style/SpinKitView.WanderingCubes"
        android:layout_width="@dimen/_30sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_gravity="center"
        app:SpinKit_Color="@color/HeaderTextColor"

        app:layout_constraintBottom_toBottomOf="@+id/Cm_image_SubCategory_CategoryScteen"
        app:layout_constraintEnd_toEndOf="@+id/Cm_image_SubCategory_CategoryScteen"
        app:layout_constraintStart_toStartOf="@+id/Cm_image_SubCategory_CategoryScteen"
        app:layout_constraintTop_toTopOf="@+id/Cm_image_SubCategory_CategoryScteen"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>